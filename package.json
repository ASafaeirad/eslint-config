{
  "name": "@frontendmonster/eslint-config",
  "version": "3.1.0",
  "license": "MIT",
  "author": "frontendmonster <frontendmonster@gmail.com>",
  "description": "frontendmonster eslint config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frontendmonster/eslint-config.git"
  },
  "files": [
    "index.js",
    "configs/rules",
    "configs/base.js",
    "configs/jest.js",
    "configs/react.js",
    "configs/typescript.js"
  ],
  "scripts": {
    "lint": "eslint .",
    "check-rules": "npm-run-all --parallel check-rules:*",
    "check-rules:base": "eslint-find-rules --unused ./configs/base.js",
    "check-rules:jest": "eslint-find-rules --unused ./configs/tests/jest.js",
    "check-rules:react": "eslint-find-rules --unused ./configs/tests/react.js",
    "check-rules:typescript": "eslint-find-rules --unused ./configs/tests/typescript.js"
  },
  "main": "index.js",
  "dependencies": {
    "@frontendmonster/utils": "0.2.10",
    "@typescript-eslint/eslint-plugin": "2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-eslint": "10.1.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jest": "23.8.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "2.5.0",
    "lodash.has": "4.5.2",
    "read-pkg-up": "7.0.1"
  },
  "peerDependencies": {
    "eslint": ">=5"
  },
  "devDependencies": {
    "eslint": "6.8.0",
    "eslint-find-rules": "3.4.0",
    "husky": "4.2.3",
    "lint-staged": "9.5.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "typescript": "3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
