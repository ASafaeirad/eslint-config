{
  "name": "@frontendmonster/eslint-config",
  "version": "4.0.4",
  "license": "MIT",
  "author": "frontendmonster <frontendmonster@gmail.com>",
  "description": "frontendmonster eslint config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frontendmonster/eslint-config.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "rules",
    "base.js",
    "index.js",
    "jest.js",
    "react.js",
    "typescript.js",
    "typecheck.js"
  ],
  "scripts": {
    "lint": "eslint .",
    "check-rules": "npm-run-all --parallel check-rules:*",
    "check-rules:base": "eslint-find-rules --unused ./base.js",
    "check-rules:jest": "eslint-find-rules --unused ./tests/jest.js",
    "check-rules:react": "eslint-find-rules --unused ./tests/react.js",
    "check-rules:typescript": "eslint-find-rules --unused ./tests/typescript.js"
  },
  "main": "index.js",
  "dependencies": {
    "@frontendmonster/utils": "0.3.9",
    "@typescript-eslint/eslint-plugin": "3.6.0",
    "@typescript-eslint/parser": "3.6.0",
    "babel-eslint": "10.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jest": "23.18.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.3",
    "eslint-plugin-react-hooks": "4.0.5",
    "lodash.has": "4.5.2",
    "read-pkg-up": "7.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.10.4",
    "eslint": "7.4.0",
    "eslint-find-rules": "3.5.0",
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "np": "6.3.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "typescript": "3.9.6"
  },
  "peerDependencies": {
    "@babel/core": ">=7",
    "eslint": ">=6",
    "prettier": ">=1",
    "react": ">=16",
    "typescript": ">=3"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    },
    "@babel/core": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
