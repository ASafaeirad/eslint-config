{
  "name": "@fullstacksjs/eslint-config",
  "version": "4.1.6",
  "license": "MIT",
  "author": "fullstacks <fullstacksjs@gmail.com>",
  "description": "fullstacks eslint config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/full-stacks/eslint-config.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "rules",
    "base.js",
    "index.js",
    "jest.js",
    "react.js",
    "typescript.js",
    "typecheck.js"
  ],
  "scripts": {
    "lint": "eslint .",
    "check-rules:base": "eslint-find-rules --unused ./base.js",
    "check-rules:jest": "eslint-find-rules --unused ./tests/jest.js",
    "check-rules:react": "eslint-find-rules --unused ./tests/react.js",
    "check-rules:typescript": "eslint-find-rules --unused ./tests/typescript.js",
    "test": "npm-run-all --parallel check-rules:*"
  },
  "main": "index.js",
  "dependencies": {
    "@babel/eslint-parser": "7.12.1",
    "@babel/eslint-plugin": "7.12.1",
    "@frontendmonster/utils": "0.3.11",
    "@typescript-eslint/eslint-plugin": "4.8.2",
    "@typescript-eslint/parser": "4.8.2",
    "eslint-config-prettier": "6.15.0",
    "eslint-import-resolver-typescript": "2.3.0",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "lodash.has": "4.5.2",
    "read-pkg-up": "7.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.12.9",
    "eslint": "7.14.0",
    "eslint-find-rules": "3.6.1",
    "husky": "4.3.0",
    "lint-staged": "10.5.2",
    "np": "7.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "typescript": "4.1.2"
  },
  "peerDependencies": {
    "@babel/core": ">=7",
    "eslint": ">=6",
    "prettier": ">=1",
    "react": ">=16",
    "typescript": ">=3"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    },
    "@babel/core": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
